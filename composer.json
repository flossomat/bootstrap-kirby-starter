{
    "name": "flossomat/bootstrap-kirby-starter",
    "description": "Bootstrap 5 + Kirby CMS Starter Template",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/flossomat/kirby-backtotop.git"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "getkirby/cms": "^3.10",
        "flossomat/kirby-backtotop": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
} 